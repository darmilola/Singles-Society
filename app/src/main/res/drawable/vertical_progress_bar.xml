<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background" android:height="2dp" android:top="8.5dp">
        <shape>
            <solid android:color="#D5E6FF"/>
            <corners android:radius="1dp"/>
        </shape>
    </item>
    <item android:id="@android:id/progress" android:height="2dp" android:top="8.5dp">
        <clip>
            <shape>
                <corners android:radius="1.5dp"/>
                <solid android:color="@color/colorPrimary"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/secondaryProgress" android:height="2dp" android:top="8.5dp">
        <clip>
            <shape>
                <corners android:radius="1dp"/>
                <solid android:color="@color/light_primary"/>
            </shape>
        </clip>
    </item>
</layer-list>